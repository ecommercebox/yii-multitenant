{
    "name": "solutosoft/yii-multitenant",
    "description": "The shared database used by all tenants",
    "keywords": ["yii2", "extension", "multitenant", "tenant"],
    "homepage": "https://gitlab.com/solutosoft/yii2-base",
    "type": "yii-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Leandro Guindani Gehlen",
            "email": "leandrogehlen@gmail.com",
            "homepage": "https://github.com/leandrogehlen"
        }
    ],
    "support": {
        "issues": "https://github.com/solutosoft/yii-multitenant/issues?state=open",
        "source": "https://github.com/solutosoft/yii-multitenant"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.16"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    },
    "autoload": {
        "psr-4": {
          "solutosoft\\multitenant\\": "src",
          "solutosoft\\multitenant\\tests\\": "tests"
        }
    }
}
